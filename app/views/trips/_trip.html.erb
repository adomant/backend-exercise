<table>
  <tbody>
      <tr>
        <td>Start city name:<%= trip.start_city_name %></td>
        <td>Station begin name:<%= trip.station_begin_name %></td>
        <td>Start time:<%= trip.start_time %></td>
        <td>End city name:<%= trip.end_city_name %></td>
        <td>Station end name:<%= trip.station_end_name %></td>
        <td>End time<%= trip.end_time %></td>
        <td>Carrier name<%= trip.carrier_name %></td>
        <td>Total cost<%= trip.total_cost %></td>
        <td>Currency<%= trip.currency %></td>
        <td><%= link_to 'Show', trip %></td>
        <td><%= link_to 'Edit', edit_trip_path(trip) %></td>
        <td><%= link_to 'Destroy', trip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  </tbody>
</table>
